<% @fishes.each do |fish| %>
  <%=  %>
<% end %>
<% fishname = [] %><%mercuryrank = [] %><%omegalevel = [] %><%foodchain = [] %><% mbarank = [] %><% mercuryppm = [] %><% sustainabiltytext = [] %>
<% image = [] %>

<!-- talks to the data.json file -->
<% JSON.parse(open("#{Rails.root}/db/data.json").read).each do |stuff| %>

<!-- calls fish name -->
<% fishname << stuff['Seafood_display_name'] %>
<% mbarank << stuff['MBA_Rank'] %>
<% omegalevel << stuff['Omega_Level'] %>
<% mercuryrank << stuff['Mercury_Rank'] %>
<% foodchain << stuff['Food_Chain'] %>
<% mercuryppm << stuff['Mercury_PPM']%>
<% sustainabiltytext << stuff['Sustainability_Seafood_Box_Text'] %>

<!-- below shows image url, but not actual image (calls from data.json, not the iamge file, need to link these together) -->
<% image << stuff['_imageurl'] %>
<% end %>
<!-- creates dropdown of all fish attr -->
<h1>Fish Name</h1>
<%= select_tag "fishname", options_for_select(fishname) %>
<h1>Sustainabilty Rank</h1>
<%= select_tag "mbarank", options_for_select(mbarank) %>
<h1>Mercury Rank</h1>
<%= select_tag "mercuryrank", options_for_select(mercuryrank) %>
<h1>Omega Level</h1>
<%= select_tag "omegalevel", options_for_select(omegalevel) %>
<h1>Food Chain Rank</h1>
<%= select_tag "foodchain", options_for_select(foodchain) %>
<h1>Mercury PPM</h1>
<%= select_tag "mercuryppm", options_for_select(mercuryppm) %>
<h1>Sustainabilty Text</h1>
<%= select_tag "sustainabiltytext", options_for_select(sustainabiltytext) %>

